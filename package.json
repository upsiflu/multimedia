{
  "private": true,
  "type": "module",
  "name": "multimedia",
  "scripts": {
    "postinstall": "elm-tooling install",
    "start-without-documentation": "run-pty % elm-watch hot % npm esbuild -- --serve=9000 --servedir=public % node dev-server.js 9000",
    "start": "run-pty run-pty.json",
    "try-production": "run-pty % esbuild --serve=9000 --servedir=public % node dev-server.js 9000",
    "build": "elm-watch make --optimize && npm esbuild",
    "esbuild": "esbuild initElm.ts --bundle --outdir=public/build --public-path=/build/ --loader:.svg=file --loader:.woff2=file --loader:.mp3=file"
  },
  "devDependencies": {
    "elm-doc-preview": "^5.0.5",
    "elm-review": "^2.10.3",
    "elm-tooling": "^1.15.0",
    "elm-watch": "1.1.2",
    "esbuild": "0.17.4",
    "run-pty": "4.0.3",
    "uglify-js": "3.17.4"
  }
}
